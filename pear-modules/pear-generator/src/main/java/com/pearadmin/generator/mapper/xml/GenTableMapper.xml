<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pearadmin.generator.mapper.GenTableMapper">

    <resultMap id="GenTableMap" type="com.pearadmin.generator.domain.GenTable">
        <id property="tableId" column="table_id"></id>
        <result property="tableNames" column="table_names"></result>
        <result property="tableComments" column="table_comments"></result>
        <result property="classNames" column="class_names"></result>
        <result property="category" column="category"></result>
        <result property="packageName" column="package_name"></result>
        <result property="moduleName" column="module_name"></result>
        <result property="businessName" column="business_name"></result>
        <result property="functionName" column="function_name"></result>
        <result property="author" column="author"></result>
        <result property="createBy" column="create_by"></result>
        <result property="createTime" column="create_time"></result>
        <result property="updateBy" column="update_by"></result>
        <result property="updateTime" column="update_time"></result>
        <result property="remark" column="remark"></result>
    </resultMap>

    <select id="selectList" parameterType="GenTable" resultMap="GenTableMap">
        select * from gen_table
    </select>

    <select id="selectById" parameterType="string" resultMap="GenTableMap">
        select * from gen_table where table_id =  #{tableId}
    </select>

    <insert id="insert" parameterType="GenTable">
        insert into gen_table
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="tableId!=null">
                table_id,
            </if>
            <if test="tableNames!=null">
                table_names,
            </if>
            <if test="tableComments!=null">
                table_comments,
            </if>
            <if test="className!=null">
                class_name,
            </if>
            <if test="packageName!=null">
                package_name,
            </if>
            <if test="moduleName!=null">
                module_name,
            </if>
            <if test="businessName!=null">
                business_name,
            </if>
            <if test="author!=null">
                author,
            </if>
            <if test="createTime!=null">
                create_time,
            </if>
        </trim>
        <trim prefix="values(" suffix=")" suffixOverrides=",">
            <if test="tableId!=null">
                #{tableId},
            </if>
            <if test="tableNames!=null">
                #{tableNames},
            </if>
            <if test="tableComments!=null">
                #{tableComments},
            </if>
            <if test="className!=null">
                #{calssName},
            </if>
            <if test="packageName!=null">
                #{packageName},
            </if>
            <if test="moduleName!=null">
                #{moduleName},
            </if>
            <if test="businessName!=null">
                #{businessName},
            </if>
            <if test="author!=null">
                #{author},
            </if>
            <if test="createTime!=null">
                #{createTime},
            </if>
        </trim>
    </insert>
</mapper>